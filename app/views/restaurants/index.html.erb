<h1>All Restaurants</h1>
<ul>
  <% @restaurants.each do |restaurant| %>
  <li>

    <%=  restaurant.name %>
    <%=  restaurant.address %>
    <%=  restaurant.phone_number %>
    <%=  restaurant.category %>
    <%= link_to "show restaurant", restaurant_path(restaurant) %>
    <%= link_to "edit restaurant", edit_restaurant_path(restaurant) %>
    <%= link_to "delete article", restaurant_path(restaurant), method: :delete %>
  </li>
  <% end %>
</ul>

<%= link_to "Add new restaurant", new_restaurant_path %>
